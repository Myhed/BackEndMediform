{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/app.js",
    "test": "jest test/unit/**/*.test.ts --passWithNoTests",
    "test:integration": "jest test/integration/*.test.ts",
    "dev": "DEBUG=esport-namespace,express* ts-node ./app.ts",
    "Bdd": "mysql -u root -p < mysql_script/BddMediform.sql",
    "procedurePatients": "mysql -u root -p  mediform < ./mysql_script/procedure/patients.sql",
    "procedureMedecins": "mysql -u root -p mediform < ./mysql_script/procedure/medecins.sql",
    "procedureRdvs": "mysql -u root -p mediform < ./mysql_script/procedure/rdv.sql",
    "procedureAffile": "mysql -u root -p mediform < ./mysql_script/procedure/affile.sql",
    "testFunctionMedecins": "mysql -u root -p mediform < ./mysql_script/testFunction/medecins.sql",
    "fakeDataPatients": "mysql -u root -p mediform < ./mysql_script/fakeData/fakeDataPatient.sql",
    "fakeDataAffile": "mysql -u root -p mediform < ./mysql_script/fakeData/fakeDataAffile.sql",
    "fakeDataMedecins": "mysql -u root -p mediform < ./mysql_script/fakeData/fakeDataMedecin.sql",
    "fakeDataRdv": "mysql -u root -p mediform < ./mysql_script/fakeData/fakeDataRdv.sql",
    "fakeDataPrend": "mysql -u root -p mediform < ./mysql_script/fakeData/fakeDataPrend.sql",
    "functionToken": "mysql -u root -p mediform < ./mysql_script/functions/token.sql",
    "functionMedecins": "mysql -u root -p mediform < ./mysql_script/functions/medecins.sql",
    "functionAffile": "mysql -u root -p mediform < ./mysql_script/functions/affile.sql",
    "functionGeneral": "mysql -u root -p mediform < ./mysql_script/functions/generic.sql",
    "allTestFunctions": "npm run testFunctionMedecins",
    "allProcedure": "npm run procedurePatients && npm run procedureMedecins && npm run procedureRdvs && npm run procedureAffile",
    "allFunction": "npm run functionToken && npm run functionGeneral && npm run functionMedecins && npm run functionAffile",
    "allFakeData": "npm run fakeDataPatients && npm run fakeDataMedecins && npm run fakeDataAffile && npm run fakeDataRdv && npm run fakeDataPrend",
    "integrateDatabase": "npm run Bdd && npm run allFunction && npm run allProcedure && npm run allTestFunctions && npm run allFakeData",
    "start:dev": "ts-node app.ts"
  },
  "author": "",
  "license": "ISC",
